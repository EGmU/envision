{
  "name": "envision",
  "version": "0.0.1-alpha",
  "description": "SiteVision CSS framework",
  "main": "index.js",
  "scripts": {
    "build-docs": "cd _docs/; jekyll build; touch ../docs/.nojekyll",
    "build-docs-js": "./node_modules/.bin/webpack --config webpack.config.dev.js",
    "build-dist": "./node_modules/.bin/webpack --config webpack.config.prod.js",
    "build": "npm-run-all build-docs-js build-docs build-dist",
    "watch": "./node_modules/.bin/webpack --config webpack.config.dev.js --watch",
    "zip-dist": "rm dist/dist.zip; zip dist.zip dist/; mv dist.zip dist/;"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sitevision/envision.git"
  },
  "keywords": [
    "Envision",
    "CSS",
    "SiteVision",
    "SDK"
  ],
  "author": "SiteVision AB",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5"
  },
  "dependencies": {
    "tether": "^1.4.0"
  }
}
